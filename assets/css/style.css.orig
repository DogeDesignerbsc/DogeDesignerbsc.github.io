body {
  font-size: 15px;
  line-height: 1.8;
  word-wrap: break-word;
  font-family: "Open Sans", sans-serif;
}

a {
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
  text-decoration: none;
}

a:hover {
