}

h1, h2, h3, h4, h5, h6 {
  margin: 0;
  font-weight: 700;
  font-family: "Dosis", sans-serif;
}

.btn.focus:focus, .btn .btn:focus {
  -webkit-box-shadow: none;
  box-shadow: none;
}

button {
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
  border: 0;
  cursor: pointer;
  background-color: transparent;
}

button:focus {
